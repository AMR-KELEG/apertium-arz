    <pardef n="noun_procilitics">
      <e><p><l/><r/></p></e>
      <e><par n="ال__det"/></e>
      <e><par n="ف__preposition"/><par n="ال__det"/></e>
      <e><par n="ع__preposition"/><par n="ال__det"/></e>
      <e><par n="ب__preposition"/></e>
      <e><par n="ب__preposition"/><par n="ال__det"/></e>
      <e><par n="ل__preposition"/></e>
      <e><par n="ل__preposition"/><p><l>ل</l><r>ال<s n="det"/><s n="def"/><j/></r></p></e>
    </pardef>

    <pardef n="preposition">
      <e><p><l></l><r><s n="pr"/></r></p></e>
    </pardef>

