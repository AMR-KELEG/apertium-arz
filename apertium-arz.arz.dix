  <!-- The lemma on it's own doesn't bear any meaning -->
  <!-- Part of the input is consumed in the <i></i> section -->
  <!-- and the rest is consumed in the <l></l> part of the paradigm -->
  <section id="main" type="standard">
    <!-- Closed classes -->
    <e lm="اللي" c="that"><p><l>اللي</l><r>اللي<s n="rel"/><s n="an"/><s n="mf"/><s n="sp"/></r></p></e>

    <e lm="محطة" c="station"><i>محط</i><par n="n__محط/ة"/></e>
    <e lm="ساعة" c="hour"><i>ساع</i><par n="n__محط/ة"/></e>
    <e lm="حاجة" c="thing"><i>حاج</i><par n="n__محط/ة"/></e>
    <e lm="رحلة" c="trip"><i>رحل</i><par n="n__محط/ة"/></e>
    <e lm="مكالمة" c="call"><i>مكالم</i><par n="n__محط/ة"/></e>
    <e lm="بطاقة" c="id card"><i>بطاق</i><par n="n__محط/ة"/></e>

    <e lm="كريم" c="cream"><i>كريم</i><par n="n__/كريم"/></e>
    <e lm="اوتوبيس" c="bus"><i>اوتوبيس</i><par n="n__/كريم"/></e>
    <e lm="مقاس" c="size"><i>مقاس</i><par n="n__/كريم"/></e>

    <e lm="اسم" c="name"><i>اسم</i><par n="n__/اسم"/></e>
    <e lm="اوضة" c="room"><i>اوض</i><par n="n__اوض/ة"/></e>
    <e lm="شنطة" c="purse"><i>شنط</i><par n="n__اوض/ة"/></e>
    <e lm="سنة" c="year"><i>سن</i><par n="n__سن/ة"/></e>

    <e lm="دقيقة" c="minute"><i>دق</i><par n="n__دق/يقة"/></e>
    <e lm="خريطة" c="map"><i>خر</i><par n="n__خر/يطة"/></e>
    <e lm="فندق" c="hotel"><i>فن</i><par n="n__فن/دق"/></e>
    <e lm="مكتب" c="desk"><i>مك</i><par n="n__مك/تب"/></e>
    <e lm="قطر" c="train"><i>قط</i><par n="n__قط/ر"/></e>
    <e lm="تذكرة" c="ticket"><i>تذ</i><par n="n__تذ/كرة"/></e>

    <e lm="حجز" c="reservation"><i>حج</i><par n="n__حج/ز"/></e>

    <e lm="يوم" c="day"><par n="n__يوم"/></e>
    <e lm="رقم" c="number"><par n="n__رقم"/></e>
    <e lm="شكل" c="shape"><par n="n__شكل"/></e>
    <e lm="نوع" c="type"><par n="n__نوع"/></e>
    <e lm="مكان" c="place"><par n="n__مكان"/></e>
    <e lm="فيلم" c="film"><par n="n__فيلم"/></e>
    <e lm="صباح" c="morning"><par n="n__صباح"/></e>
  </section>

  <!-- Inconditionals -->
  <section id="final" type="inconditional">
    <e><par n="numeros_arabigos"/></e>
    <e><re>[.\?;:!…¿¶]</re><p><l></l><r><s n="sent"/></r></p></e>
    <e><re>[،,]</re><p><l></l><r><s n="cm"/></r></p></e>
  </section>

</dictionary>

