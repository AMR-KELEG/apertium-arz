  <!-- The lemma on it's own doesn't bear any meaning -->
  <!-- Part of the input is consumed in the <i></i> section -->
  <!-- and the rest is consumed in the <l></l> part of the paradigm -->
  <section id="main" type="standard">
    <!-- Closed classes -->
    <e lm="اللي" c="that"><p><l>اللي</l><r>اللي<s n="rel"/><s n="an"/><s n="mf"/><s n="sp"/></r></p></e>
    <e lm="انا" c="me"><p><l>انا</l><r>prpers<s n="prn"/><s n="p1"/><s n="mf"/><s n="sg"/></r></p></e>
    <e lm="احنا" c="me"><p><l>احنا</l><r>prpers<s n="prn"/><s n="p1"/><s n="mf"/><s n="pl"/></r></p></e>
    <e lm="انت" c="you"><p><l>انت</l><r>prpers<s n="prn"/><s n="p2"/><s n="mf"/><s n="sg"/></r></p></e>
    <e lm="انتم" c="you"><p><l>انتم</l><r>prpers<s n="prn"/><s n="p2"/><s n="mf"/><s n="pl"/></r></p></e>
    <e lm="هو" c="he"><p><l>هو</l><r>prpers<s n="prn"/><s n="p3"/><s n="m"/><s n="sg"/></r></p></e>
    <e lm="هى" c="she"><p><l>هى</l><r>prpers<s n="prn"/><s n="p3"/><s n="f"/><s n="sg"/></r></p></e>
    <e lm="هم" c="he"><p><l>هم</l><r>prpers<s n="prn"/><s n="p3"/><s n="mf"/><s n="pl"/></r></p></e>

    <e lm="ده" c="this"><p><l>ده</l><r>ده<s n="prn"/><s n="def"/><s n="m"/><s n="sg"/></r></p></e>
    <e lm="ديه" c="this"><p><l>ديه</l><r>ده<s n="prn"/><s n="def"/><s n="f"/><s n="sg"/></r></p></e>
    <e lm="دول" c="these"><p><l>دول</l><r>ده<s n="prn"/><s n="def"/><s n="mf"/><s n="pl"/></r></p></e>

    <e lm="ده" c="this"><p><l>ده</l><r>ده<s n="det"/><s n="dem"/><s n="m"/><s n="sg"/></r></p></e>
    <e lm="ديه" c="this"><p><l>ديه</l><r>ده<s n="det"/><s n="dem"/><s n="f"/><s n="sg"/></r></p></e>
    <e lm="دول" c="these"><p><l>دول</l><r>ده<s n="det"/><s n="dem"/><s n="mf"/><s n="pl"/></r></p></e>

    <!-- Prepositions -->
    <e lm="من" c="from"><i>من</i><par n="preposition"/></e>
    <e lm="الى" c="to"><i>الى</i><par n="preposition"/></e>
    <e lm="في" c="in"><i>في</i><par n="preposition"/></e>
    <e lm="على" c="on"><i>على</i><par n="preposition"/></e>
    <e lm="مع" c="with"><i>مع</i><par n="preposition"/></e>
    <e lm="عن" c="about"><i>عن</i><par n="preposition"/></e>
    <e lm="ل" c="for"><i>ل</i><par n="preposition"/></e>
    <e lm="ماعدا" c="except"><i>ماعدا</i><par n="preposition"/></e>
    <e lm="خلال" c="through"><i>خلال</i><par n="preposition"/></e>
    <e lm="قبل" c="before"><i>قبل</i><par n="preposition"/></e>
    <e lm="بعد" c="after"><i>بعد</i><par n="preposition"/></e>
    <e lm="قدام" c="in front of"><i>قدام</i><par n="preposition"/></e>
    <e lm="وسط" c="in the middle of"><i>وسط</i><par n="preposition"/></e>
    <e lm="ورا" c="behind"><i>ورا</i><par n="preposition"/></e>

    <!-- We need to add the <a/> to activate the post-generator module -->
    <!-- The following rule is used only in the right to left direction -->
    <e lm="ال" r="RL"><p><l><a/>ال</l><r>ال<s n="det"/><s n="def"/></r></p></e>

    <e lm="محطة" c="station"><par n="noun_procilitics"/><i>محط</i><par n="n__محط/ة"/></e>
    <e lm="عربية" c="car"><par n="noun_procilitics"/><i>عربي</i><par n="n__محط/ة"/></e>
    <e lm="ساعة" c="hour"><par n="noun_procilitics"/><i>ساع</i><par n="n__محط/ة"/></e>
    <e lm="حاجة" c="thing"><par n="noun_procilitics"/><i>حاج</i><par n="n__محط/ة"/></e>
    <e lm="رحلة" c="trip"><par n="noun_procilitics"/><i>رحل</i><par n="n__محط/ة"/></e>
